<!-- title method is defind in app/helpers/application_helper-->
<%= title "Todo Lists" %>

<% if @todo_lists.size > 0 %>
    <ul class = "list todo-lists no-bullet">
        <%= render partial: "todo_list", collection: @todo_lists%>
    </ul>
<% else %>
    <%= render partial: "blanl_slate" %>
<% end %>